'''def calculate_factorial(n):
  if n<0:
    return("factorial of negative number is not possible")
  elif n==0:
    return 1
  else:
    return(n*calculate_factorial(n-1))
user_input=(int(input("Enter an integer:")))
print(calculate_factorial(user_input))'''


'''def find_divisors(n):
  divisors=[]
  for i in range(1,n+1):
    if n%i==0:
      divisors.append(i)
  return divisors
user_input=int(input("Enter an integer:"))
print(find_divisors(user_input))'''


'''def reverseString(input_str):
  reversed_str=""
  for char in input_str:
    reversed_str=char+reversed_str

  return reversed_str
user_input=input("Enter a string:")
print(reverseString(user_input))'''


'''def filter_even_numbers(input_list):
  even_numbers=[]
  for number in input_list:
    if number%2==0:
      even_numbers.append(number)
  return even_numbers
user_input=input("Enter a list of numbers:")
print(filter_even_numbers(user_input))'''



'''def check_password(password):
  if len(password)<8:
    return "Weak password"
  elif not any(char.isdigit() for char in password):
    return "Weak password"
  elif not any(char.isupper() for char in password):
    return "Weak password"
  elif not any(char.islower() for char in password):
    return "Weak password"
  else:
    return "Strong password"
user_input=input("Enter a password:")
print(check_password(user_input)) '''
